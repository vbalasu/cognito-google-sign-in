{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "ListYourObjects",
          "Effect": "Allow",
          "Action": "s3:ListBucket",
          "Resource": ["arn:aws:s3:::profile.cloudmatica.com"],
          "Condition": {
              "StringLike": {
                  "s3:prefix": ["cognito/profile/${cognito-identity.amazonaws.com:sub}"]
              }
          }
      },
      {
          "Sid": "ReadWriteDeleteYourObjects",
          "Effect": "Allow",
          "Action": [
              "s3:GetObject",
              "s3:PutObject",
              "s3:DeleteObject"
          ],
          "Resource": [
              "arn:aws:s3:::profile.cloudmatica.com/cognito/profile/${cognito-identity.amazonaws.com:sub}",
              "arn:aws:s3:::profile.cloudmatica.com/cognito/profile/${cognito-identity.amazonaws.com:sub}/*"
          ]
      }
  ]
}